<section id="merchant-stats-box">
	<ul id="top-three-revenue">
		<% @top_three_revenue.each do |merchant| %>
			<li> <%= merchant.name %> </li>
		<% end %>
	</ul>

	<ul id="fastest-merchants">
		<% @fastest_merchants.each do |merchant| %>
			<li> <%= merchant.name %> </li>
		<% end %>
	</ul>

	<ul id="slowest-merchants">
		<% @slowest_merchants.each do |merchant| %>
			<li> <%= merchant.name %> </li>
		<% end %>
	</ul>

</section>

<% @active_merchants.each do |merchant| %>
	<ul id="merchant-<%= merchant.id %>-info" >
	<% if current_admin? %>
		<li><%= link_to merchant.name, admin_merchant_path(merchant) %></li>
	<% else %>
		<li><%= merchant.name %></li>
	<% end %>
		<li><%= merchant.city %></li>
		<li><%= merchant.state %></li>
		<li><%= merchant.created_at.strftime("%m-%d-%Y") %></li>
	</ul>
<% end %>
