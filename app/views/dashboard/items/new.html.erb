<%= form_for @item do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :image %>
  <%= f.text_field :image %>

  <%= f.label :price %>
  <%= f.text_field :price %>

  <%= f.label :inventory, "Available Inventory" %>
  <%= f.text_field :inventory %>

  <%= f.submit "Add Item" %>

<% end %>
